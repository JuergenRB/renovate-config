{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableDependencyDashboard",
    ":automergeMinor",
    ":label(dependency)"
  ],
  "cake": {
    "addLabels": [
      "cake",
      "build"
    ]
  },
  "github-actions": {
    "addLabels": [
      "github_actions",
      "build"
    ]
  },
  "nuget": {
    "addLabels": [
      ".net"
    ]
  },
  "commitMessagePrefix": "(maint)"
}
